{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "tsc": "tsc",
    "build": "npm run test:build && webpack --config webpack.config.production.js ",
    "test": "mocha-webpack --watch --webpack-config webpack.config.test.js \"tests/**/index.tsx\"",
    "test:build": "mocha-webpack --webpack-config webpack.config.test.js \"tests/**/index.tsx\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon) , Aykut Alpgiray Ate≈ü (http://github.com/aalpgiray)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-es2015-native-modules": "^6.9.3",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.1",
    "mocha-webpack": "^0.5.0",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "ts-loader": "^0.8.2",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-notifier": "^1.3.2"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "babel-polyfill": "^6.13.0",
    "immutable": "^3.8.1",
    "lodash": "^4.14.1",
    "material-ui": "^0.15.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-alpha.3",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.5.2",
    "redux-promise-middleware": "^3.3.2",
    "typescript": "^2.1.0-dev.20160805"
  }
}
